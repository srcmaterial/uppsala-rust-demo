  </rdf:Description>

  <rdf:Description rdf:about="http://example.org/character/Aragorn">
    <dc:title>Aragorn</dc:title>
    <ex:race>Human</ex:race>
    <ex:title>King of Gondor</ex:title>
  </rdf:Description>

</rdf:RDF>